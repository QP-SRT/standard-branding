<div id="payment_methods" class="col-md-8">
  <ul>
    {% for method in model.payment_methods %}
      <li>
      <a href="{{ model.session_id }}/payment-method/{{ method.name }}">
          <img src="{{ method.image_path }}" alt="{{ method.nice_name }}" title="{{ method.nice_name }}"/>
          {{ method.nice_name }}
        </a>
      </li>
    {% endfor %}
  </ul>
</div>
