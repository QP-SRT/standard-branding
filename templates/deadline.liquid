<div class="well-xs well-blueish-xs well-sm well-blueish-sm" data-session-id="{{ model.session_id }}">
  <div class="container">
    <div class="row">
      <div class="col-xs-12">
        <h3 class="center-all"><i class="fa fa-fw fa-3x fa-info-circle"></i> {{ lang.deadline.header }}</h3>
      </div>
      <div class="col-xs-12">
        <p>{{ lang.deadline.message }}
          {% if model.cancel_url %}
            <a href="{{ model.cancel_url }}">{{ lang.deadline.continue }}</a>.
          {% endif %}
        </p>
      </div>
    </div>
  </div>
</div>
