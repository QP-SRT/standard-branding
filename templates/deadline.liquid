<div class="well-xs well-blueish-xs well-sm well-blueish-sm" data-session-id="{{ model.session_id }}">
  <div class="container">
    <div class="row">
      <div class="col-xs-12">
        <h3 class="center-xy"><i class="fa fa-fw fa-3x fa-info-circle"></i> {% t Order expired %}}</h3>
      </div>
      <div class="col-xs-12">
        <p>{% t You did not finish the purchase within the time frame. %}
          {% if model.cancel_url %}
            <a href="{{ model.cancel_url }}">{% t Click here to continue %}</a>.
          {% endif %}
        </p>
      </div>
    </div>
  </div>
</div>
